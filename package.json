{
	"name": "react-hawksearch",
	"version": "0.0.1",
	"description": "Hawksearch React Library",
	"author": "Hawksearch",
	"license": "Proprietary",
	"private": true,
	"scripts": {
		"clean": "rimraf ./dist/ && mkdirp ./dist",
		"check:lint": "tslint --project .",
		"check:types": "tsc --project .",
		"check:style": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"build:only": "npm run clean && webpack",
		"build:dev": "npm run check:lint && npm run check:types && npm run check:style && npm test && npm run clean && webpack",
		"build:prod": "npm run check:lint && npm run check:types && npm run check:style && npm test && npm run clean && webpack -p",
		"start": "npm run clean && webpack-dev-server --open",
		"analyze-bundle": "npm run clean && webpack -p --profile --json > ./dist/bundle.json && webpack-bundle-analyzer ./dist/bundle.json"
	},
	"devDependencies": {
		"@babel/cli": "^7.1.5",
		"@babel/core": "^7.1.6",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.1.6",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.1.0",
		"@types/history": "^4.7.2",
		"@types/html-webpack-plugin": "^3.2.0",
		"@types/jest": "^23.3.10",
		"@types/mini-css-extract-plugin": "^0.2.0",
		"@types/node": "^10.12.10",
		"@types/react-dom": "^16.0.11",
		"@types/react-hot-loader": "^4.1.0",
		"@types/react-test-renderer": "^16.0.3",
		"@types/rheostat": "^2.1.4",
		"@types/webpack": "^4.4.19",
		"@types/webpack-bundle-analyzer": "^2.13.0",
		"@types/webpack-dev-server": "^3.1.1",
		"acorn": "^6.1.1",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^23.6.0",
		"babel-loader": "^8.0.4",
		"css-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^24.7.1",
		"mini-css-extract-plugin": "^0.5.0",
		"mkdirp": "^0.5.1",
		"node-sass": "^4.11.0",
		"prettier": "^1.17.0",
		"react-hot-loader": "^4.3.12",
		"react-test-renderer": "^16.8.6",
		"rimraf": "^2.6.2",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"ts-node": "^7.0.1",
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.17.0",
		"tslint-react": "^3.6.0",
		"tslint-react-hooks": "^2.0.0",
		"typescript": "^3.2.1",
		"webpack": "^4.30.0",
		"webpack-bundle-analyzer": "^3.3.2",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.3.1"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"downshift": "^3.1.5",
		"history": "^4.7.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"rheostat": "^3.0.2"
	}
}
