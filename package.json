{
	"name": "react-hawksearch",
	"version": "0.0.1",
	"description": "Hawksearch React Library",
	"private": true,
	"scripts": {
		"clean": "rimraf ./dist/ && mkdirp ./dist",
		"lint": "tslint --project .",
		"typecheck": "tsc --project .",
		"test": "jest",
		"test:watch": "jest --watch",
		"build:dev": "npm run lint && npm run typecheck && npm test && npm run clean && webpack",
		"build:prod": "npm run lint && npm run typecheck && npm test && npm run clean && webpack -p",
		"start": "npm run clean && webpack-dev-server --open",
		"analyze-bundle": "npm run clean && webpack -p --profile --json > ./dist/bundle.json && webpack-bundle-analyzer ./dist/bundle.json"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.1.5",
		"@babel/core": "^7.1.6",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.1.6",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.1.0",
		"@types/history": "^4.7.2",
		"@types/html-webpack-plugin": "^3.2.0",
		"@types/jest": "^23.3.10",
		"@types/node": "^10.12.10",
		"@types/react-dom": "^16.0.11",
		"@types/react-hot-loader": "^4.1.0",
		"@types/react-test-renderer": "^16.0.3",
		"@types/webpack": "^4.4.19",
		"@types/webpack-bundle-analyzer": "^2.13.0",
		"@types/webpack-dev-server": "^3.1.1",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^23.6.0",
		"babel-loader": "^8.0.4",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^23.6.0",
		"mkdirp": "^0.5.1",
		"react-hot-loader": "^4.3.12",
		"react-test-renderer": "^16.7.0-alpha.2",
		"rimraf": "^2.6.2",
		"ts-node": "^7.0.1",
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.17.0",
		"tslint-react": "^3.6.0",
		"typescript": "^3.2.1",
		"webpack": "^4.26.1",
		"webpack-bundle-analyzer": "^3.0.3",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.10"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"downshift": "^3.1.5",
		"history": "^4.7.2",
		"react": "^16.7.0-alpha.2",
		"react-dom": "^16.7.0-alpha.2"
	}
}
